{
  "name": "reliqarts/laravel-guided-image",
  "description": "Simplified and ready image manipulation for Laravel via intervention image.",
  "keywords": [
    "image",
    "route",
    "generation",
    "laravel",
    "photo",
    "laravel5",
    "resize",
    "thumb",
    "dummy",
    "crop"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "reliq",
      "email": "reliq@reliqarts.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "illuminate/support": "6 - 8",
    "intervention/image": "^2.4",
    "intervention/imagecache": "^2.3",
    "reliqarts/laravel-common": "^5.4",
    "ext-json": "*",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "orchestra/testbench": "4 - 6",
    "phpro/grumphp": "^1.0",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpunit/phpunit": "^9.3",
    "symplify/easy-coding-standard": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "ReliqArts\\GuidedImage\\": "src/",
      "ReliqArts\\GuidedImage\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test:ci": "phpunit --verbose --colors=auto --coverage-clover=coverage.xml",
    "test:unit": "phpunit --testsuite=Unit --verbose --coverage-clover=coverage.xml"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "ReliqArts\\GuidedImage\\ServiceProvider"
      ]
    }
  }
}
